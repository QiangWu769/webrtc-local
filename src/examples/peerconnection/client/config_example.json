{
  "video_source": {
    "camera": {
      "enabled": false
    },
    "video_file": {
      "enabled": true,
      "file_path": "/path/to/your/video.yuv",
      "width": 640,
      "height": 480,
      "fps": 30
    },
    "video_disabled": {
      "enabled": false
    }
  },
  
  "video_output": {
    "enabled": true,
    "file_path": "/path/to/output/received_video.yuv",
    "width": 640,
    "height": 480,
    "fps": 30
  },
  
  "logging": {
    "level": "info",
    "save_to_file": true,
    "log_output_path": "/path/to/logs/webrtc_client.log"
  },
  
  "auto_close_on_completion": true,
  "transmission_time_seconds": 20
}